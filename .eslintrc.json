{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "max-len": 1,
    "no-multi-spaces": 0,
    "no-trailing-spaces": 0,
    "object-curly-spacing": 0,
    "no-tabs": 0,
    "no-unused-expressions": 0,
    "no-sequences": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-await-in-loop": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0
  },
  "globals": {
    "artifacts": "readonly",
    "describe": "readonly",
    "beforeEach": "readonly",
    "it": "readonly"
  }
}
